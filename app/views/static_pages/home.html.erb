<div class="banner">
  <div class="books">
    <div class="book-1"></div>
    <div class="book-2"></div>
    <div class="book-3"></div>
  </div>
  <div class="stand"></div>
</div>
<div class="categories">
  <div class="container">
    <div class="col-md-4">
      <div class="category category-1">
